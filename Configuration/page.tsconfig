TCEMAIN {
  translateToMessage = %s

  table.pages {
    # Pages will *not* have "(copy)" appended:
    disablePrependAtCopy = 1
    # Pages will *not* be hidden upon copy:
    disableHideAtCopy = 1
  }

  table.tt_content {
    # Content will *not* have "(copy)" appended:
    # disablePrependAtCopy = 1
    # Content will *not* be hidden upon copy:
    disableHideAtCopy = 1
  }
}
